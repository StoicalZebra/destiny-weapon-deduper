{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "src/main.ts",
    "src/**/*.test.ts",
    "e2e/**/*.spec.ts"
  ],
  "project": ["src/**/*.{ts,vue}", "e2e/**/*.ts"],
  "ignore": [
    "dump_perks.ts",
    "src/components/playground/**",
    "src/services/playground-mock-service.ts"
  ],
  "ignoreDependencies": [
    "@vue/test-utils",
    "@vitest/coverage-v8"
  ],
  "ignoreExportsUsedInFile": true,
  "rules": {
    "enumMembers": "off"
  }
}
